{
  "name": "@funnel-io/lalr",
  "version": "0.0.13",
  "main": "lib/index.js",
  "license": "MIT",
  "bin": {
    "lalr": "./lib/index.js"
  },
  "scripts": {
    "prepack": "yarn build",
    "copy-adapters": "cp -r ./src/adapters ./lib/",
    "build": "esbuild --define:process.env.VERSION=\\\"${npm_package_version}\\\" src/*.ts --bundle --outdir=lib --platform=node --external:fsevents --external:nodemon && yarn copy-adapters"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.95",
    "@types/nodemon": "^1.19.1",
    "commander": "^9.2.0",
    "fastify": "^3.28.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "esbuild": "^0.14.38",
    "nodemon": "^2.0.15"
  },
  "files": [
    "lib/adapters"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/funnel-io/lalr"
  }
}
